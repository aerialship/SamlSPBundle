{
    "name": "aerialship/saml-sp-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony2 SAML2 authentication bundle",
    "autoload": {
        "psr-0": { "AerialShip\\SamlSPBundle\\": "src/" }
    },
    "require": {
        "php": ">=5.4.1",
        "aerialship/lightsaml": "~1.0",
        "symfony/symfony": "~2.2"
    },
    "suggest": {
        "doctrine/orm": "If you want store sso state to database.",
        "doctrine/mongodb-odm": "If you want store sso state to database using mongodb."
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
